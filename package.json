{
  "expo": {
    "name": "SEHAT",
    "slug": "sehat",
    "privacy": "public",
    "version": "1.1.0",
    "Platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "sdkVersion": "39.0.4",
    "icon": "./assets/images/new/icon.png",
    "splash": {
      "image": "./assets/images/new/splash.png",
      "resizeMode": "contain"
    }
  },
  "ios": {
    "bundleIdentifier": "com.medical_app.SEHAT",
    "buildNumber": "1.1.0",
    "supportsTablet": true
  },
  "android": {
    "package": "com.medical_app.SEHAT",
    "VersionCode": 1
  },
  "permissions": [
    "ACCESS_FINE_LOCATION",
    "ACCESS_BACKGROUND_LOCATION",
    "CAMERA",
    "READ_EXTERNAL_STORAGE",
    "ACCESS_WIFI_STATE",
    "ACCESS_NETWORK_STATE"
  ],
  "assetBundlePatterns": [
    "**/*"
  ],
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "main": "node_modules/expo/AppEntry.js",
  "splash": {
    "image": "./assets/images/new/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#00e676"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.2",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/datetimepicker": "3.0.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.7",
    "@react-native-community/picker": "^1.8.1",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/material-bottom-tabs": "^5.3.10",
    "@react-navigation/material-top-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.21.1",
    "eslint": "^7.17.0",
    "expo": "^40.0.0",
    "expo-asset": "~8.2.1",
    "expo-constants": "~9.3.3",
    "expo-font": "~8.4.0",
    "expo-image-picker": "~9.2.0",
    "expo-linking": "~2.0.0",
    "expo-network": "~2.4.0",
    "expo-splash-screen": "~0.8.1",
    "expo-status-bar": "~1.0.3",
    "expo-web-browser": "~8.6.0",
    "faker": "^5.1.0",
    "formik": "^2.2.6",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "native-base": "^2.15.2",
    "randomcolor": "^0.6.2",
    "react": "^17.0.1",
    "react-devtools": "^4.10.1",
    "react-dom": "16.13.1",
    "react-native": "^0.63.4",
    "react-native-base64": "^0.2.1",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-get-random-values": "^1.5.1",
    "react-native-image-crop-picker": "^0.35.2",
    "react-native-image-zoom-viewer": "^3.0.1",
